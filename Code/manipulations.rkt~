;; a)

(define (rotate-right lst)
  (local
    [(define (last-element lst)
       (first (foldl cons empty lst)))
     (define (all-but-last lst)
       (foldl cons empty (rest (foldl cons empty lst))))]
    (cons (last-element lst) (all-but-last lst))))

